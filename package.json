{
  "name": "mpc-hc-website",
  "version": "0.1.5",
  "author": "MPC-HC Team (https://github.com/mpc-hc/website)",
  "description": "The dependencies to build MPC-HC's website",
  "homepage": "https://github.com/mpc-hc/website",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpc-hc/website.git"
  },
  "bugs": {
    "url": "https://github.com/mpc-hc/website/issues"
  },
  "license": "MIT",
  "main": "make.js",
  "readmeFilename": "Readme.md",
  "scripts": {
    "check": "node run-tests",
    "lint": "node run-tests"
  },
  "dependencies": {
    "clean-css": "~1.1.1",
    "csslint": "~0.9.10",
    "jshint": "~2.1.10",
    "shelljs": "~0.2.5",
    "uglify-js": "~2.4.0"
  },
  "jshintConfig": {
    "bitwise": true,
    "boss": true,
    "browser": false,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "indent": 4,
    "latedef": true,
    "multistr": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "plusplus": false,
    "quotmark": "double",
    "regexp": true,
    "shelljs": true,
    "strict": true,
    "trailing": true,
    "unused": true
  },
  "engines": {
    "node": "~0.8.0"
  }
}
